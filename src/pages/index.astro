---
import Layout from '../layouts/Layout.astro';
import CardsPage from "../components/CardsPage.astro"
import CardPopUp from "../components/CardPopUp.astro"


const sampleEvents = [
    { eventName: 'Wired:\nWiCS Dev Project', imagePath: '/src/assets/Group80.png' },
    { eventName: 'Aspire to Inspire:\nMentorship Program', imagePath: '/src/assets/Group80.png' },
    { eventName: 'Women in\nTech Week', imagePath: '/src/assets/Group80.png' }
];
---

<Layout>
	<CardPopUp eventInfo="This semester long project allows particpants to collaborate in teams to design and develop a website, providing a hands-on opportunity to enhance technical skills and gain real-world experience."/>
    <CardsPage events={sampleEvents}/>
</Layout>
