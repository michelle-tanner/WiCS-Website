---
import Layout from '../layouts/Layout.astro';
import Nav3 from '../components/Nav.astro';
import Footer from '../components/Footer.astro';
import Sublanding from '../components/Sublanding.astro';
import Resource from '../components/Resource.astro';


const section = [
   {title : 'Internship List 2024',
    links: [
        { label: 'Internship List 2024', href: '#'},
        { label: 'Summer 2024 Internship List', href: '#'},
]
   },
   {
    title: 'Resume guide',
    links: [
        { label: 'Ins and Outs of a resume', href: '#'},
        { label: 'Example Resumes', href: '#'},    
    ]
   },
   

    {
        title: 'Workshop Slides Archive', 
        links: [
            { label: 'Build Your Personal Website', href: '#'},
            { label: 'Intro to APIs', href: '#'},
            { label: 'Linkedin Workshop', href: '#'},
            { label: 'Hack the Gap - Git Workshop', href: '#'},

        ]
    },
    {
        title:'Other Resources', 
        links: [
            { label: 'Cs student guide', href: '#'}
        ]
    }

]


const orgs = [
    {name: 'Society of Women Engineers' }
]
---
    
    <Nav3/>
    <Layout/>
    <Sublanding title="Resources", subheadline="Be a part of a diverse community dedicated to empowering women in tech!"/>
    {section.map(sec => (
        <Resource title={sec.title} links={sec.links} />
      ))}


    <Footer/>
</Layout>